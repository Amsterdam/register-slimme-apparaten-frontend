    "@datapunt/matomo-tracker-js": "0.0.15",
    "@formatjs/intl-pluralrules": "^1.5.9",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "axios": "^0.21.1",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "connected-react-router": "^6.8.0",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "hoist-non-react-statics": "^3.3.2",
    "immutable": "^3.8.2",
    "ip": "^1.1.5",
    "leaflet": "^1.7.1",
    "leaflet.markercluster": "^1.5.0",
    "lodash": "^4.17.20",
    "minimist": "1.2.5",
    "proj4": "^2.6.3",
    "proj4leaflet": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-albus": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-intl": "5.20.7",
    "react-loadable": "^5.5.0",
    "react-media": "^1.10.0",
    "react-reactive-form": "^1.0.32",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "recharts": "^1.4.2",
    "redux": "^4.1.0",
    "redux-immutable": "4.0.0",
    "redux-saga": "^1.1.1",
    "reselect": "^4.0.0",
    "styled-components": "^5.3.0",
    "warning": "^4.0.3",
    "whatwg-fetch": "^2.0.4",
    "typescript": "^4.3.5"
