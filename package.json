    "@formatjs/intl-pluralrules": "^1.5.2",
    "@types/jest": "^25.1.3",
    "axios": "^0.19.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "connected-react-router": "^6.5.2",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "hoist-non-react-statics": "^3.3.2",
    "ip": "^1.1.5",
    "leaflet": "^1.6.0",
    "leaflet.markercluster": "^1.4.1",
    "lodash": "^4.17.15",
    "minimist": "1.2.0",
    "proj4": "^2.6.0",
    "proj4leaflet": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-accessible-accordion": "^2.4.4",
    "react-albus": "^2.0.0",
    "react-dom": "^16.10.2",
    "react-intl": "2.3.0",
    "react-loadable": "^5.5.0",
    "react-media": "^1.10.0",
    "react-reactive-form": "^1.0.32",
