    "mini-css-extract-plugin": "^0.8.2",
    "node-plop": "^0.26.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "null-loader": "^3.0.0",
    "offline-plugin": "^5.0.7",
    "open": "^7.3.0",
    "plop": "^2.7.4",
    "pre-commit": "1.2.2",
    "prettier": "^2.3.2",
    "prettier-eslint": "^13.0.0",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.4",
    "style-loader": "^1.3.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
